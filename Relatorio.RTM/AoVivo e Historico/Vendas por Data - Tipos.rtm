          '     (NOTA_TIPOS_1.ID_NOTA_TIPO = NOTA_1.ID_NOTA_TIPO)'
          'WHERE (( CUPOM2_1.NPEDIDO = 0 )'
          '       OR ( CUPOM2_1.NPEDIDO IS NULL  )'
