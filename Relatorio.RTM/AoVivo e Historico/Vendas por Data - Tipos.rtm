        SQLText.Strings = (
          'SELECT NOTA_TIPOS_1.NOME, '
          '       SUM(CUPOM2_1.SUBTOTAL) SUM_CUPOM2_1_SUBTOTAL, '
          '       SUM(CUPOM2_1.DESCONTO) SUM_CUPOM2_1_DESCONTO, '
          '       SUM(CUPOM2_1.ACRECIMO) SUM_CUPOM2_1_ACRECIMO, '
          '       SUM(CUPOM2_1.TOTAL) SUM_CUPOM2_1_TOTAL, '
          '       SUM(CUPOM2_1.TROCO) SUM_CUPOM2_1_TROCO'
